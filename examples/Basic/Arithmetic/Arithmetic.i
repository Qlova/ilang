software {
	var a = num(reada(' '))
	var b = num(reada('\n'))
	
	output("Sum: ")
	output(text( a+b ))
	output("\n")
	
	output("Difference: ")
	output(text( a-b ))
	output("\n")
	
	output("Product: ")
	output(text( a*b ))
	output("\n")
	
	output("Quotient: ")
	output(text( a/b ))
	output("\n")
	
	output("Modulus: ")
	output(text( a mod b ))
	output("\n")

	output("Exponent: ")
	output(text( a^b ))
	output("\n")
}
